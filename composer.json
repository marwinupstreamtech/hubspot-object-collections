{
    "name": "marwinupstreamtech/hubspot-custom-fields",
    "description": "PHP package for the HubSpot V3",
    "type": "package",
    "homepage": "https://github.com/marwinupstreamtech/hubspot-custom-fields",
    "version": "0.1",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.4",
        "firebase/php-jwt": "^6.2",
        "league/oauth2-client": "^2.6",
        "nesbot/carbon": "^2.58"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "barryvdh/laravel-ide-helper": "^2.13"
    },
    "autoload": {
        "psr-4": {
            "HubSpot\\HubSpotSDK\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HubSpot\\HubSpotCRMCollection\\HubSpotServiceProvider"
            ],
            "aliases": {
                "HubSpotCRMCollection": "HubSpot\\HubSpotCRMCollection\\HubSpotCRMCollection"
            }
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Marwin Intal",
            "email": "marwin@upstreamtech.io"
        }
    ],
    "minimum-stability": "stable"
}
